<div class="usersList">
    <h2>Transactions for account number <%= @accounts.account_number%> for user <%= @user.username %></h2>
    <table class="listing" >
      <tr class="header">
        <th>date</th>
        <th>Transaction Type</th>
        <th >Amount</th>
      </tr>
      <% @transactions.each do |t| %>
      <tr>
        <td><%= t.created_at %></td>
        <td><%= t.transactionType%></td>
        <td><%= @accounts.currency + t.amount.to_s%> </td>
        <td class="actions">
            <%= button_to 'Delete',@accounts.id.to_s + '/'+ t.id.to_s + "/delete" , :method=> "get"  %>
       </td>
      </tr>
      <% end %>
    </table>
  </div>


   